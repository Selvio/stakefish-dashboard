{
  "name": "stakefish-dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "polished": "^4.1.4",
    "react": "^17.0.2",
    "react-bootstrap-icons": "^1.7.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"./**/*.{ts,tsx}\" --max-warnings 0 --ignore-path .gitignore",
    "test:e2e": "start-server-and-test start http://localhost:3000 cypress:run",
    "cypress:run": "cypress run --config-file cypress.json",
    "test:e2e-open": "start-server-and-test start http://localhost:3000 cypress:open",
    "cypress:open": "cypress open --config-file cypress.json",
    "deploy": "npm run build && mv build/index.html build/200.html && surge build http://stakefish-dashboard.surge.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "cypress": "^9.5.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "msw": "^0.42.1",
    "start-server-and-test": "^1.14.0"
  }
}
